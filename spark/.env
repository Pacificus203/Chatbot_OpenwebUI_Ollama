# ==================================================
# Ollama GPU
# ==================================================
OLLAMA_GPU_DRIVER=cpu
OLLAMA_GPU_COUNT=1

# ===============================
# OpenTelemetry
# ===============================
ENABLE_OTEL=true
ENABLE_OTEL_TRACES=true
ENABLE_OTEL_METRICS=true
OTEL_EXPORTER_OTLP_ENDPOINT=http://grafana:4317
OTEL_EXPORTER_OTLP_INSECURE=true
OTEL_SERVICE_NAME=open-webui
OTEL_RESOURCE_ATTRIBUTES=service.namespace=codelab,env=dev

# ==================================================
# ---------------------------
# Google Drive Integration
# ---------------------------

# ENABLE_GOOGLE_DRIVE_INTEGRATION: True để bật, False để tắt
ENABLE_GOOGLE_DRIVE_INTEGRATION=False

# GOOGLE_DRIVE_CLIENT_ID: Client ID từ Google Cloud Console, cần bật Drive API & Picker API
GOOGLE_DRIVE_CLIENT_ID=

# GOOGLE_DRIVE_API_KEY: API Key từ Google Cloud Console
GOOGLE_DRIVE_API_KEY=

# ---------------------------
# OneDrive Integration
# ---------------------------

# ENABLE_ONEDRIVE_INTEGRATION: True để bật toàn bộ OneDrive integration, False để tắt
ENABLE_ONEDRIVE_INTEGRATION=False

# Personal OneDrive option (cá nhân)
ENABLE_ONEDRIVE_PERSONAL=True
ONEDRIVE_CLIENT_ID_PERSONAL=

# Work/School OneDrive option (doanh nghiệp)
ENABLE_ONEDRIVE_BUSINESS=True
ONEDRIVE_CLIENT_ID_BUSINESS=

# Legacy generic Client ID (khuyến nghị không dùng)
ONEDRIVE_CLIENT_ID=

# SharePoint configuration cho Work/School OneDrive
ONEDRIVE_SHAREPOINT_URL=
ONEDRIVE_SHAREPOINT_TENANT_ID=

