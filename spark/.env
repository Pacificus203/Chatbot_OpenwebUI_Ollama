# Elasticsearch
# ==================================================
ELASTICSEARCH_API_KEY=zciqgEEbTjyE1VEgUTz48Q
ELASTICSEARCH_CA_CERTS=
ELASTICSEARCH_CLOUD_ID=
ELASTICSEARCH_INDEX_PREFIX=open_webui_collections
ELASTICSEARCH_PASSWORD=Q3c5iarNaqaJtuYVBf5A
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_USERNAME=elastic
# ==================================================
# Ollama Settings
# ==================================================
OLLAMA_NUM_THREADS=4
OLLAMA_GPU_LAYERS=100
OLLAMA_KV_CACHE=4GB
OLLAMA_MAX_LOADED_MODELS=10
OLLAMA_NUM_PARALLEL=10

# ==================================================
# Open WebUI Core
# ==================================================
VERSION=main
CONTAINER_NAME=openwebui
HOST_IP=0.0.0.0
WEBUI_PORT=3030
PORT=8080

WEBUI_SECRET_KEY=a06f9c0a9ed63702c94ad60201f705eb68ebb6c9abb4315f427039bc5e35b88e
WEBUI_NAME=NTC AI Assistant

APP_PATH=./data
DATA_DIR=/data

CPUS=0
MEMORY_LIMIT=0

# ==================================================
# Security
# ==================================================
ENABLE_PERSISTENT_CONFIG=true

# ==================================================
# Authentication & Permissions
# ==================================================
ENABLE_SIGNUP=true
ENABLE_SIGNUP_PASSWORD_CONFIRMATION=true
ENABLE_LOGIN_FORM=true
ENABLE_PASSWORD_AUTH=true
ENABLE_OAUTH_SIGNUP=true

ENABLE_ADMIN_EXPORT=true
ENABLE_ADMIN_CHAT_ACCESS=true
BYPASS_ADMIN_ACCESS_CONTROL=true

ENABLE_USER_WEBHOOKS=true
ENABLE_API_KEYS=true

# ==================================================
# API Providers
# ==================================================
ENABLE_OLLAMA_API=true
OLLAMA_BASE_URL=http://ollama:11434

ENABLE_OPENAI_API=true
OPENAI_API_BASE_URL=https://api.openai.com/v1
# OPENAI_API_KEY=sk-xxxx

# ==================================================
# Networking
# ==================================================
ENABLE_WEBSOCKET_SUPPORT=true

# ==================================================
# Redis
# ==================================================
REDIS_SOCKET_CONNECT_TIMEOUT=5
REDIS_URL=redis://redis:6379/0

# ==================================================
# Database
# ==================================================
DATABASE_URL=postgres://openwebui:openwebui@postgres:5432/openwebui

# ==================================================
# Logging & Audit
# ==================================================
GLOBAL_LOG_LEVEL=INFO
AUDIT_LOG_LEVEL=NONE
AUDIT_LOGS_FILE_PATH=${DATA_DIR}/audit.log

# ==================================================
# Features
# ==================================================
ENABLE_IMAGE_GENERATION=true
ENABLE_CODE_EXECUTION=true
ENABLE_CODE_INTERPRETER=true
ENABLE_AUTOCOMPLETE_GENERATION=true
ENABLE_PUBLIC_ACTIVE_USERS_COUNT=true

# ==================================================
# Advanced
# ==================================================
ENABLE_VERSION_UPDATE_CHECK=false
UVICORN_WORKERS=128
# OFFLINE_MODE=false
# CACHE_CONTROL=no-store, max-age=0

# ==================================================
# Custom Model
# ==================================================
ENABLE_CUSTOM_MODEL_FALLBACK=false
